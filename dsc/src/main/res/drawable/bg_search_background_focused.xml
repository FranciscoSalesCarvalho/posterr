<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="@dimen/line_lg">
        <shape android:shape="rectangle">
            <solid android:color="@color/support200" />
            <corners android:radius="@dimen/size_xsm" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/line_lg"
        android:gravity="bottom"
        android:left="@dimen/size_2xsm"
        android:right="@dimen/size_2xsm">
        <shape android:shape="rectangle">
            <size android:height="@dimen/line_lg" />
            <solid android:color="@color/primary200" />
            <corners
                android:bottomLeftRadius="@dimen/size_xsm"
                android:bottomRightRadius="@dimen/size_xsm"
                />
        </shape>
    </item>
</layer-list>